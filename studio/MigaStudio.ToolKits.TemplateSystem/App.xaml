<windows:ModernApp x:Class="Acorisoft.FutureGL.MigaStudio.ToolKits.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Acorisoft.FutureGL.MigaStudio.ToolKits"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             xmlns:windows="https://www.acorisoft.cn/miga/windows"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemesDictionary Theme="Light" />
                <ResourceDictionary Source="pack://application:,,,/Wpf.Ui;component/Styles/Controls/ScrollViewer.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MigaUI;component/Themes/Generic.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Wpf.Ui;component/Styles/Controls/ScrollBar.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MigaUI.Windows;component/Themes/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</windows:ModernApp>
